        <complexType name="stringItemType">
            <simpleContent>
                <restriction base="xbrli:stringItemType"/>
            </simpleContent>
        </complexType>
        <complexType name="amountItemType">
            <simpleContent>
                <restriction base="xbrli:monetaryItemType"/>
            </simpleContent>
        </complexType>
        <complexType name="binaryObjectItemType">
