	<element name="NC00-NC61" id="NC00-NC61" abstract="true" type="xbrli:stringItemType" nillable="true" substitutionGroup="xbrli:item" xbrli:periodType="instant"/>
	<element name="NC61" id="NC61" abstract="true" type="xbrli:stringItemType" nillable="true" substitutionGroup="xbrli:item" xbrli:periodType="instant"/>
	<element name="NC61-01" id="NC61-01" type="cor:identifierItemType" nillable="false" substitutionGroup="xbrli:item" xbrli:periodType="instant"/>
	<element name="NC61-03" id="NC61-03" type="cor:amountItemType" nillable="false" substitutionGroup="xbrli:item" xbrli:periodType="instant"/>
	<element name="NC63-04" id="NC63-04" type="cor:quantityItemType" nillable="false" substitutionGroup="xbrli:item" xbrli:periodType="instant"/>
